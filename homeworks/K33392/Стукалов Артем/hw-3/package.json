{
  "name": "hw2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently --kill-others -n swc-build,run-server \"yarn build-watch\" \"sleep 1 && yarn dev-watch\"",
    "dev-watch": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "build-watch": "yarn build -w",
    "build": "swc ./src --strip-leading-paths --config-file ./.swcrc -d dist",
    "clean": "rimraf ./dist",
    "preview": "cross-env NODE_ENV=production node ./dist/main.js",
    "lint": "yarn eslint --ext .ts,tsx --max-warnings=0 ./src",
    "format": "yarn eslint --ext .ts,tsx --fix ./src",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@prisma/client": "^5.10.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-type-provider-zod": "^1.1.9",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4",
    "zod-prisma-types": "^3.1.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.2",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
